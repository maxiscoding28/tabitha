{
  "name": "Tabitha",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "html/popup.html"
  },
  "permissions": ["tabs", "tabGroups", "sidePanel"],
  "background": {
    "service_worker": "js/service-worker.js"
  },
  "icons": {
    "16": "icons/tabitha16.png",
    "48": "icons/tabitha48.png",
    "128": "icons/tabitha128.png"
  }
}
